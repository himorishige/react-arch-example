{
  "name": "reactjs-vite-tailwindcss-boilerplate",
  "description": "This is a React + vite boilerplate to be used with Tailwindcss.",
  "version": "0.1",
  "private": true,
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "test": "vitest",
    "coverage": "vitest run --coverage",
    "test:ui": "vitest --ui",
    "lint": "eslint src --max-warnings=0",
    "lint:css": "stylelint 'src/**/*.@(css|scss)'",
    "type-check": "tsc --noEmit",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "dependencies": {
    "@tanstack/react-location": "^3.7.0",
    "axios": "^0.26.1",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "react-error-boundary": "^3.1.4",
    "react-query": "^4.0.0-alpha.20",
    "regenerator-runtime": "^0.13.9"
  },
  "devDependencies": {
    "@storybook/addon-a11y": "^6.4.22",
    "@storybook/addon-actions": "^6.4.22",
    "@storybook/addon-essentials": "^6.4.22",
    "@storybook/addon-links": "^6.4.22",
    "@storybook/addon-postcss": "^2.0.0",
    "@storybook/builder-vite": "^0.1.30",
    "@storybook/react": "^6.4.22",
    "@storybook/testing-react": "^1.2.4",
    "@tanstack/react-location-devtools": "^3.4.4",
    "@testing-library/jest-dom": "^5.16.1",
    "@testing-library/react": "^13.1.1",
    "@testing-library/react-hooks": "^7.0.2",
    "@types/jest": "^27.4.0",
    "@types/node": "^17.0.8",
    "@types/react": "^17.0.38",
    "@types/react-dom": "^17.0.11",
    "@types/regenerator-runtime": "^0.13.1",
    "@types/testing-library__jest-dom": "^5.14.2",
    "@typescript-eslint/eslint-plugin": "^5.9.1",
    "@typescript-eslint/parser": "^5.9.1",
    "@vitejs/plugin-react": "^1.1.4",
    "autoprefixer": "^10.4.2",
    "esbuild-register": "^3.3.2",
    "eslint": "^8.6.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "eslint-plugin-storybook": "^0.5.11",
    "eslint-plugin-tailwindcss": "^3.1.2",
    "eslint-plugin-unused-imports": "^2.0.0",
    "jest-extended": "^1.2.0",
    "jest-watch-typeahead": "^1.0.0",
    "jsdom": "^19.0.0",
    "msw": "^0.39.2",
    "msw-storybook-addon": "^1.6.3",
    "postcss": "^8.4.5",
    "prettier": "2.5.1",
    "react-test-renderer": "^17.0.2",
    "stylelint": "^14.2.0",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-recess-order": "^3.0.0",
    "stylelint-config-recommended-scss": "^5.0.2",
    "stylelint-config-standard": "^24.0.0",
    "stylelint-order": "^5.0.0",
    "tailwindcss": "^3.0.12",
    "typescript": "^4.5.4",
    "vite": "^2.9.5",
    "vite-plugin-env-compatible": "^1.1.1",
    "vite-plugin-svgr": "^1.0.1",
    "vite-tsconfig-paths": "^3.3.17",
    "vitest": "^0.8.0"
  },
  "msw": {
    "workerDirectory": "public"
  }
}
